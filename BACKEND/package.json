{
  "name": "backend-posts",
  "version": "1.0.0",
  "type": "module",
  "description": "Proyecto de publicacion y aceptacion de notas del usuario",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "biome check --error-on-warnings .",
    "format:check": "biome format --error-on-warnings .",
    "validate": "npm run check && npm run format:check",
    "check:fix": "biome check --apply .",
    "format": "biome format --write .",
    "fix": "npm run check:fix && npm run format"
  },
  "author": "Ignaccio7",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "5.1.1",
    "cloudinary": "2.2.0",
    "cors": "2.8.5",
    "dompurify": "^3.2.2",
    "dotenv": "^16.4.0",
    "express": "4.18.2",
    "jsdom": "^25.0.1",
    "jsonwebtoken": "9.0.2",
    "multer": "1.4.5-lts.1",
    "mysql2": "3.9.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.3"
  }
}
